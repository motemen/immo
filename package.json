  "bugs": {
    "url": "https://github.com/motemen/immo/issues"
  },
  "homepage": "https://github.com/motemen/immo#readme",
  "devDependencies": {
    "chai": "^3.2.0",
    "dtsm": "^0.10.3",
    "mocha": "^2.2.5",
    "nexpect": "^0.5.0",
    "typescript": "^1.5.3"
  },
  "dependencies": {
    "lodash.assign": "^3.2.0",
    "minimist": "^1.1.3"
  }
}
