{
  "name": "immo",
  "version": "0.1.0",
  "description": "Runs a command and retry on failure",
  "main": "lib/immo.js",
  "bin": {
    "immo": "./bin/immo"
  },
  "scripts": {
    "build": "tsc src/lib/cli.ts --module commonjs --outDir lib src/lib/cli.ts",
    "watch": "tsc src/lib/cli.ts --watch --module commonjs --outDir lib src/lib/cli.ts",
    "setup": "dtsm --insight false install"
  },
